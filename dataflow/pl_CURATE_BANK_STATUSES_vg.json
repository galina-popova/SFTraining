{
	"name": "pl_CURATE_BANK_STATUSES_vg",
	"properties": {
		"folder": {
			"name": "curration"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_ingestion_BankStatuses_vg",
						"type": "DatasetReference"
					},
					"name": "ingBankStatusesVg"
				},
				{
					"dataset": {
						"referenceName": "ds_curration_BankStatuses_vg",
						"type": "DatasetReference"
					},
					"name": "CurBankStatusesVg"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_ingestion_BankStatuses_vg",
						"type": "DatasetReference"
					},
					"name": "AddNewRecords"
				},
				{
					"dataset": {
						"referenceName": "ds_curration_BankStatuses_vg",
						"type": "DatasetReference"
					},
					"name": "updatedRecords"
				}
			],
			"transformations": [
				{
					"name": "joinIngestionAndCuration"
				},
				{
					"name": "RenameColumns"
				},
				{
					"name": "splitNewAndUpdatedRecords"
				},
				{
					"name": "UpdateRecords"
				}
			],
			"scriptLines": [
				"source(output(",
				"          BankStatusId as integer,",
				"          BankStatusCode as string,",
				"          BankStatusName as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> ingBankStatusesVg",
				"source(output(",
				"          BankStatusId as integer,",
				"          BankStatusCode as string,",
				"          BankStatusName as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> CurBankStatusesVg",
				"ingBankStatusesVg, CurBankStatusesVg join(ingBankStatusesVg@BankStatusCode == CurBankStatusesVg@BankStatusCode,",
				"     joinType:'left',",
				"     broadcast: 'auto')~> joinIngestionAndCuration",
				"joinIngestionAndCuration select(mapColumn(",
				"          ING_BankStatusId = ingBankStatusesVg@BankStatusId,",
				"          ING_BankStatusCode = ingBankStatusesVg@BankStatusCode,",
				"          ING_BankStatusName = ingBankStatusesVg@BankStatusName,",
				"          CUR_BankStatusId = CurBankStatusesVg@BankStatusId,",
				"          CUR_BankStatusCode = CurBankStatusesVg@BankStatusCode,",
				"          CUR_BankStatusName = CurBankStatusesVg@BankStatusName",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> RenameColumns",
				"RenameColumns split(isNull(CUR_BankStatusCode),",
				"     and(ING_BankStatusCode==CUR_BankStatusCode, ING_BankStatusName!=CUR_BankStatusName),",
				"     disjoint: false) ~> splitNewAndUpdatedRecords@(newRecords, updatedRecords, notChanged)",
				"splitNewAndUpdatedRecords@updatedRecords alterRow(updateIf(true())) ~> UpdateRecords",
				"splitNewAndUpdatedRecords@newRecords sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          BankStatusId as integer,",
				"          BankStatusCode as string,",
				"          BankStatusName as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          BankStatusCode = ING_BankStatusCode,",
				"          BankStatusName = ING_BankStatusName",
				"     )) ~> AddNewRecords",
				"UpdateRecords sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          BankStatusId as integer,",
				"          BankStatusCode as string,",
				"          BankStatusName as string",
				"     ),",
				"     deletable:false,",
				"     insertable:false,",
				"     updateable:true,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> updatedRecords"
			]
		}
	}
}