						"type": "DatasetReference"
					},
					"name": "InsertNewRecords"
				},
				{
					"dataset": {
						"referenceName": "ds_curration_customer_vg",
						"type": "DatasetReference"
					},
					"name": "InsertUpdatedRecords"
				},
				{
					"dataset": {
						"referenceName": "ds_curration_customer_vg",
						"type": "DatasetReference"
					},
					"name": "UpdateExisting"
