{
	"name": "ds_CURRATE_PAYMENT_STATUSES",
	"properties": {
		"folder": {
			"name": "curration"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_ingestion_PaymentStatuses_vg",
						"type": "DatasetReference"
					},
					"name": "ingPaymentStatuses"
				},
				{
					"dataset": {
						"referenceName": "ds_ingestion_PaymentStatuses_vg",
						"type": "DatasetReference"
					},
					"name": "currPaymentStatus"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_curration_PaymentStatuses_vg",
						"type": "DatasetReference"
					},
					"name": "curationTargetPaymentStatusesVg"
				}
			],
			"transformations": [
				{
					"name": "RecordDoesntExists"
				}
			],
			"scriptLines": [
				"source(output(",
				"          PaymentStatusId as integer,",
				"          PaymentStatusName as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> ingPaymentStatuses",
				"source(output(",
				"          PaymentStatusId as integer,",
				"          PaymentStatusName as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> currPaymentStatus",
				"ingPaymentStatuses, currPaymentStatus exists(ingPaymentStatuses@PaymentStatusName == currPaymentStatus@PaymentStatusName,",
				"     negate:true,",
				"     broadcast: 'left')~> RecordDoesntExists",
				"RecordDoesntExists sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          PaymentStatusId as integer,",
				"          PaymentStatusName as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          PaymentStatusName",
				"     )) ~> curationTargetPaymentStatusesVg"
			]
		}
	}
}